      "target": "api::pattern.pattern"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "categories": {
      "type": "customField",
      "default": "[]",
      "options": [
        "the_entrepreneur",
        "time_space",
        "sustainability_responsibility",
        "team_collaboration",
        "customers_stakeholders_ecosystem",
        "the_solution"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "phases": {
      "type": "customField",
      "default": "[]",
      "options": [
        "start",
        "discover_explore",
        "transform",
        "create",
        "implement"
      ],
      "customField": "plugin::multi-select.multi-select"
    }
  }
}
